{
  "css.customData": ["./vscode.tailwind.json"],
  "eslint.validate": ["javascript", "javascriptreact", "astro", "typescript", "typescriptreact"],
  "files.associations": {
    "*.mdx": "markdown"
  },
  "prettier.documentSelectors": ["**/*.astro"],
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "yaml.schemas": {
    "./.vscode/astrowind/config-schema.json": "/src/config.yaml"
  },
  "eslint.useFlatConfig": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ffffff",
    "activityBar.background": "#ffffff",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#df9f9f",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#ffffff",
    "statusBar.background": "#fefff4",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#f9ffc1",
    "statusBarItem.remoteBackground": "#fefff4",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#fefff4",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#fefff499",
    "titleBar.inactiveForeground": "#15202b99",
    "sideBar.border": "#ffffff"
  },
  "peacock.remoteColor": "#FEFFF4"
}
